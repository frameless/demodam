{
  "name": "@frameless/demodam",
  "version": "1.0.0",
  "description": "Website voor demodam",
  "main": "gatsby-config.js",
  "scripts": {
    "lint": "npm-run-all lint:** ",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "lint-fix": "npm-run-all lint-fix:**",
    "lint-fix:eslint": "eslint --fix .",
    "lint-fix:prettier": "prettier --write .",
    "develop": "gatsby develop -H 0.0.0.0",
    "start": "npm run develop",
    "build": "gatsby build --prefix-paths",
    "serve": "gatsby serve -H 0.0.0.0 n--prefix-paths",
    "clean": "gatsby clean",
    "prepare": "husky install",
    "theme-build": "style-dictionary build --config style-dictionary.config.json",
    "prebuild": "npm run theme-build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frameless/demodam.git"
  },
  "author": "Community for NL Design System",
  "license": "EUPL-1.2",
  "bugs": {
    "url": "https://github.com/frameless/demodam/issues"
  },
  "homepage": "https://github.com/frameless/demodam#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "clsx": "1.2.1",
    "eslint": "8.33.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-react": "7.32.2",
    "husky": "8.0.3",
    "is-absolute-url": "4.0.1",
    "lint-staged": "13.1.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.3",
    "style-dictionary": "3.7.2",
    "stylelint-order": "5.0.0",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "@fontsource/source-sans-pro": "4.5.11",
    "@utrecht/component-library-react": "1.0.0-alpha.247",
    "@utrecht/design-tokens": "1.0.0-alpha.431",
    "gatsby": "5.5.0",
    "gatsby-plugin-image": "3.7.0",
    "gatsby-plugin-sharp": "5.7.0",
    "gatsby-source-filesystem": "5.7.0",
    "gatsby-transformer-sharp": "5.7.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
}
